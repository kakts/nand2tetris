/**
 * Or gate:
 * out = (((a == 1) || (b == 1))), 1, 0) 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    Not (in=a, out=o1);
    Not (in=b, out=o2);
    Nand (a=o1, b=o2, out=out);
}
